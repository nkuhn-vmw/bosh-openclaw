---
# Approved skills from ClawHub
# Only skills on this list will be synced and served by the registry

skills:
<% p('openclaw.registry.skills_allowlist').each do |skill| %>
  - <%= skill %>
<% end %>

security:
  scan_on_sync: <%= p('openclaw.registry.scan_on_sync') %>
  block_network_calls: <%= p('openclaw.registry.block_network_calls') %>
  require_signature: false
  quarantine_on_fail: true
  scan_checks:
    - manifest_present
    - signature_valid
    - no_unauthorized_network
    - no_dangerous_patterns
  dangerous_patterns:
    - child_process
    - eval(
    - Function(
    - exec(
    - spawn(
  allowed_network_modules: []
